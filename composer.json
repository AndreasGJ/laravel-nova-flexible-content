{
  "name": "gwd/laravel-nova-flexible-content",
  "description": "A Laravel Nova field.",
  "keywords": [
    "laravel",
    "nova",
    "nova-field"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.1.0"
  },
  "autoload": {
    "psr-4": {
      "Gwd\\FlexibleContent\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Gwd\\FlexibleContent\\FieldServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
